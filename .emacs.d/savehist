;; -*- mode: emacs-lisp; coding: utf-8-unix -*-
;; Minibuffer history file, automatically generated by ‘savehist’.

(setq savehist-minibuffer-history-variables '(file-name-history extended-command-history))
(setq file-name-history '("~/PowerShell/.emacs.d/" "~/Powershell" "~/PowerShell/.emacs.d/init.el" "~/PowerShell/.emacs.d/emacs-custom.el"))
(setq extended-command-history '("vc-dir" "recentf-open-files"))
(setq kill-ring '("PowerShell/.emacs.d/" #("(recentf-mode t)" 0 16 (fontified t))))
(setq search-ring '(#("back" 0 4 (isearch-regexp-function nil isearch-case-fold-search t))))
(setq regexp-search-ring 'nil)
